{
    "name": "webvicli-all",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "npm run start-express",
        "start-simple": "cd Builds/Simple_Simple && webvicli",
        "start-express": "cd Builds/Server_HelloWorld && webvicli",
        "start-electron": "electron .",
        "start-robot": "cd Builds/RobotTest_Robot && webvicli",
        "build": "electron-packager . --out=dist/ --overwrite --electron-version=6.0.4"
    },
    "dependencies": {
        "webvicli": "file:packages/webvicli",
        "webvicli-electron": "file:packages/webvicli-electron",
        "webvicli-express": "file:packages/webvicli-express",
        "webvicli-node-fs": "file:packages/webvicli-node-fs",
        "webvicli-node-path": "file:packages/webvicli-node-path"
    },
    "optionalDependencies": {
        "webvicli-robot": "file:packages/webvicli-robot"
    }
}
